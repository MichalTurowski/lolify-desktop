{
    "QMainWindow": [
        {
            "tittle": "Lolify",
            "icon":":/font_awesome_brands/icons/font_awesome/brands/windows.png",
			"frameless": true,
			"transluscentBg": true,
			"navigation":[
				{
					"minimize": "minimize_window",
					"close": "close_window",
					"restore":[
						{	
							"buttonName": "restore_window",
							"normalIcon": ":/feather/icons/feather/square.png",
							"maximizedIcon": ":/feather/icons/feather/copy.png"
						}
					],
					"moveWindow": "header",
					"tittleBar": "header"
				}
			]
        }
    ],
    "QSettings": [
        {
            "ThemeSettings": [
                {
                    "CustomTheme": [
                        {
                            "Background-color": "#F5F5F5",
                            "Text-color": "#070808",
                            "Accent-color": "#0F9696",
                            "Icons-color": "#333333",
                            "Theme-name": "Dark-Blue",
                            "Default-Theme": true
                        },
						{
                            "Background-color": "#16191d",
                            "Text-color": "#F5F5F5",
                            "Accent-color": "#03C3C3",
                            "Icons-color": "#F5F5F5",
                            "Theme-name": "Light-Blue",
                            "Default-Theme": false
                        }
                    ]
                }
            ],
            "AppSettings": {
                "OrginizationName": "Lolify",
                "ApplicationName": "Lolify Desktop",
                "OrginizationDormain": "lolify.vercel.app"
            }
        }
    ],
    "QCustomQStackedWidget":[
		{
			"name":"stackedWidget_2",
			"navigation":[
				{
					"navigationButtons":[
						{
                            "topBtn": "page_3",
                            "championsBtn": "page_4"
						}
					]
				}
			],
            "transitionAnimation":[
				{
					"slide":[
						{
							"active": true,
							"duration":1000,
                            "direction": "horizontal",
							"easingCurve": "OutBack"
						}
					]
				}
			]
		}
	],
    "QPushButtonGroup": [{
		"Buttons": [
			"topBtn",
			"championsBtn",
			"searchBtn",
			"profileBtn"
		],
		"Style":[{
			"Active": "background-color: $COLOR_ACCENT_3;",
			"NotActive": ""
		}]
	}],
	"QCustomCheckBox":[
		{
			"name":"checkBox_app",
			"bgColor":"#DCDCDC",
			"circleColor":"#339999",
			"activeColor":"#99CCCC",
			"animationEasingCurve":"InOutCubic",
			"animationDuration":300
		}
	],
    "QCustomSlideMenu": [{
		"name": "leftMenuContainer",
		"defaultSize": [{
			"width": 52,
			"height": "parent"
		}],
		"collapsedSize": [{
			"width": 52,
			"height": "parent"
		}],
		"expandedSize": [{
			"width": 160,
			"height": "parent"
		}],
		"toggleButton": [{
			"buttonName": "menuBtn",
			"icons": [{
				"whenMenuIsCollapsed": ":/feather/icons/feather/align-justify.png",
				"whenMenuIsExpanded": ":/feather/icons/feather/chevron-left.png"
			}]
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 500,
			"animationEasingCurve": "Linear"
		}]
	},
    {
		"name": "centerMenuContainer",
		"defaultSize": [{
			"width": 0,
			"height": "parent"
		}],
		"collapsedSize": [{
			"width": 0,
			"height": "parent"
		}],
		"expandedSize": [{
			"width": 200,
			"height": "parent"
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 500,
			"animationEasingCurve": "Linear"
		}]
	},
    {
        "name": "notificationSlide",
        "defaultSize": [
            {
                "width": 0,
                "height": "auto"
            }
        ],
        "collapsedSize": [
            {
                "width": 0,
                "height": "auto"
            }
        ],
        "expandedSize": [
            {
                "width": 500,
                "height": 100
            }
        ],
        "toggleButton": [
            {
                "buttonName": "closeNotification"
            }
        ],
        "menuTransitionAnimation": [
            {
                "animationDuration": 400,
                "animationEasingCurve": "Linear"
            }
        ],
		"floatPosition": [
			{
				"relativeTo": "centralwidget",
				"position": "center-center",
				"shadow":[
					{
						"color": "#000",
						"blurRadius": 20,
						"xOffset": 0,
						"yOffset": 0
					}
				]
			}
		]
    }
    ]
}
